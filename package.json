{
  "name": "AlinmaPay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint --ext .tsx --ext .ts app/ .",
    "lint:fix": "eslint --ext .tsx --ext .ts app/ . --fix",
    "format": "prettier --write ./app",
    "start": "react-native start",
    "test": "jest",
    "docs": "jsdoc -c jsdoc.conf.json",
    "ios:dev": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayDev\" ",
    "ios:prod": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayProd\" ",
    "ios:preprod": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayPreProd\" ",
    "ios:uat": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayUat\" ",
    "ios:sit": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPaySit\" ",
    "android:prod": "react-native run-android --mode=proddebug --appId=com.alinmapay.consumer.mobile",
    "android:preprod": "react-native run-android --mode=preproddebug --appId=com.alinmapay.consumer.mobile.preprod",
    "android:dev": "react-native run-android --mode=devdebug --appId=com.alinmapay.consumer.mobile.dev",
    "android:uat": "react-native run-android --mode=uatdebug --appId=com.alinmapay.consumer.mobile.uat",
    "android:sit": "react-native run-android --mode=sitdebug --appId=com.alinmapay.consumer.mobile.sit",
    "storybook-generate": "sb-rn-get-stories",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{json,md}": [
      "prettier --write"
    ]
  },
